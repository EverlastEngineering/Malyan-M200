    360          void plan_init() {
    361            block_buffer_head = block_buffer_tail = 0;
    362            memset(position, 0, sizeof(position)); // clear position
    363            for (int i = 0; i < NUM_AXIS; i++) previous_speed[i] = 0.0;
    364            previous_nominal_speed = 0.0;
    365          }
